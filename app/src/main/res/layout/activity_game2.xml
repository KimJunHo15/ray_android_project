<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/game2_cl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="false"
    tools:context=".game2">

    <ImageView
        android:id="@+id/game2_img_1"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="136dp"
        android:layout_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline9"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_2"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_1"
        app:layout_constraintTop_toTopOf="@+id/game2_img_1"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_3"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_2"
        app:layout_constraintTop_toTopOf="@+id/game2_img_2"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_4"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_3"
        app:layout_constraintTop_toTopOf="@+id/game2_img_3"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_5"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toStartOf="@+id/game2_img_1"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_1"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_7"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_6"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_2"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_6"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_5"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_1"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_8"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_7"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_3"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_10"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_9"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_5"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_9"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toStartOf="@+id/game2_img_5"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_5"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_11"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_10"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_6"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_12"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_11"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_7"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_13"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toStartOf="@+id/game2_img_9"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_9"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_14"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_13"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_9"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_15"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_14"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_10"
        app:srcCompat="@drawable/big_logo" />

    <ImageView
        android:id="@+id/game2_img_16"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_weight="1"
        app:layout_constraintStart_toEndOf="@+id/game2_img_15"
        app:layout_constraintTop_toBottomOf="@+id/game2_img_11"
        app:srcCompat="@drawable/big_logo" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15" />

    <Button
        android:id="@+id/btn_test_return"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="돌아가기"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_count_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:text="5"
        android:textSize="60dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_game2_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="0"
        android:textSize="48dp"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/pro_game2"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:max="180"
        android:progressDrawable="@drawable/progressbar_progressbar1"
        android:scrollbarSize="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="최고점수"
        android:textSize="28dp"
        app:layout_constraintStart_toStartOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="108dp"
        android:text="현재점수"
        android:textSize="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_game2_best"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="0"
        android:textSize="48dp"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/tv_game2_score" />
</androidx.constraintlayout.widget.ConstraintLayout>