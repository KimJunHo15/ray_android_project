        <activity
            android:name=".game1"
            android:exported="false" />
        <activity
            android:name=".game3"
            android:exported="false" />
        <activity
            android:name=".game2"
            android:exported="false" />
        <activity
            android:name=".game4"
            android:exported="false" />
        <activity
            android:name=".Exam_camera"
            android:exported="false" />
        <activity
            android:name=".Exam_upload"
            android:exported="false" />
        <activity android:name=".FoodFragment"
